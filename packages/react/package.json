{
  "name": "@sankyu/react-circle-flags",
  "version": "1.6.1",
  "description": "400+ circular SVG React flags â€” tree-shakeable, TypeScript-ready, SSR-compatible, zero deps.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "predev": "node ../../scripts/life-cycle.js build-core --pkg react",
