{
  "name": "@sankyu/circle-flags-core",
  "version": "1.6.1",
  "description": "Shared core utilities for circle-flags packages",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "node ../../scripts/life-cycle.js ensure-generated --pkg core",
    "build": "tsup",
    "postbuild": "node ../../scripts/tasks/postbuild-core.mjs",
    "dev": "tsup --watch",
    "pretypecheck": "node ../../scripts/life-cycle.js ensure-generated --pkg core",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SanKyu-Lab/circle-flags-ui.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/SanKyu-Lab/circle-flags-ui/issues"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
